   "source": [
    "MenOfPassion(A[], n) {\n",
    "    sum <- 0;\n",
    "    for i <- 1 to n-2\n",
    "        for j <- i+1 to n-1\n",
    "            for k <- j +1 to n\n",
    "                sum <- sum + A[i] * A[j] * A[k];\n",
    "    return sum;\n",
    "}"
   ]
