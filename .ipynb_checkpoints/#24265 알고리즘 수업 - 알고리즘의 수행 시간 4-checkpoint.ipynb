   "source": [
    "MenOfPassion(A[], n) {\n",
    "    sum <- 0;\n",
    "    for i <- 1 to n-1\n",
    "        for j <- i + 1 to n\n",
    "            sum <- sum + A[i] * A[j];\n",
    "    return sum;\n",
    "}\n",
    "\n",
    "## 실행 : ((n+1)*n)/2"
   ]
  },
